void fUNCTION(string s){
      
        int a[999999];
        a[0]=0;
        a[1]=0;
        int i=0,j=1;
        while(j<s.length()&& i<j){
         if(s[i]==s[j]){i++;
             a[j]=i;
             j++;
         }   
         else{
             if(i!=0){
                 i=a[i-1];
             }
             else{
                 a[j]=0;
                 j++;
             }
         }
        }
        cout<<a[s.length()-1]<<endl;
    }
	
